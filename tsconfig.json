{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "noImplicitAny": true,
    "removeComments": false,
    "sourceMap": true,
    "noEmitOnError": true,
    "strictNullChecks": true,
    "noUnusedParameters": true,
    "outDir": "built"
  }

}
